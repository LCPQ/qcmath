   RowBox[{"Module", "[", "\n", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "maxSCF", ",", "thresh", ",", "nBas2", ",", "nO2", ",", "nV2", ",", 
       "soERI", ",", "so\[Epsilon]", ",", "\[Epsilon]O", ",", "\[Epsilon]V", 
       ",", "\[CapitalDelta]", ",", "OOOO", ",", "OOVV", ",", "OVOV", ",", 
