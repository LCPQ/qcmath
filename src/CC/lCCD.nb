               "}"}], "]"}], "]"}], ";"}], "\n", "    ", ",", "\n", "    ", 
           RowBox[{
            RowBox[{"PrintTemporary", "[", 
             RowBox[{"TableForm", "[", 
              RowBox[{"{", 
               RowBox[{"{", 
