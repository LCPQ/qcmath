             "\n", "    ", 
            RowBox[{"nDIIS", "=", 
             RowBox[{"Min", "[", 
              RowBox[{
               RowBox[{"nDIIS", "+", "1"}], ",", "maxDIIS"}], "]"}]}], ";", 
