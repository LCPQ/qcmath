               RowBox[{"-", 
                FractionBox["r2", "\[CapitalDelta]"]}], ",", "t"}], "]"}]}], 
            ";", "\n", "    ", 
            RowBox[{
             RowBox[{"eDIIS", "\[LeftDoubleBracket]", 
              RowBox[{"All", ",", 
               RowBox[{"1", ";;", "nDIIS"}]}], "\[RightDoubleBracket]"}], "=", 
             RowBox[{"tmpErrorDIIS", "\[LeftDoubleBracket]", 
              RowBox[{"All", ",", 
               RowBox[{"1", ";;", "nDIIS"}]}], "\[RightDoubleBracket]"}]}], 
            ";", "\n", "    ", 
            RowBox[{
             RowBox[{"FDIIS", "\[LeftDoubleBracket]", 
              RowBox[{"All", ",", 
               RowBox[{"1", ";;", "nDIIS"}]}], "\[RightDoubleBracket]"}], "=", 
             RowBox[{"tmpF", "\[LeftDoubleBracket]", 
              RowBox[{"All", ",", 
               RowBox[{"1", ";;", "nDIIS"}]}], "\[RightDoubleBracket]"}]}], 
            ";", 
            RowBox[{"Print", "[", 
             RowBox[{"t", "\[LeftDoubleBracket]", 
              RowBox[{"1", ",", "1", ",", "1", ",", "1"}], 
              "\[RightDoubleBracket]"}], "]"}], ";"}]}], "\n", "  ", "]"}], 
         ";", "\n", "  ", "\n", "  ", 
         RowBox[{"If", "[", 
          RowBox[{"verbose", ",", "\n", "    ", 
           RowBox[{
            RowBox[{"Print", "[", 
             RowBox[{"TableForm", "[", 
              RowBox[{"{", 
               RowBox[{"{", 
                RowBox[{"nSCF", ",", 
                 RowBox[{"SetPrecision", "[", 
                  RowBox[{"ElCCD", ",", "15"}], "]"}], ",", 
                 RowBox[{"SetPrecision", "[", 
