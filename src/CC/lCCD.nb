      RowBox[{"so\[Epsilon]", "=", 
       RowBox[{"SpatialToSpinMOEnergy", "[", 
        RowBox[{"nBas", ",", "nO", ",", "nV", ",", "\[Epsilon]"}], "]"}]}], 
      ";", "\n", 
      RowBox[{"soERI", "=", 
       RowBox[{"SpatialToSpinERI", "[", 
        RowBox[{"nBas", ",", "nO", ",", "nV", ",", "ERI"}], "]"}]}], ";", 
      "\n", "\n", 
      RowBox[{"(*", 
       RowBox[{"Antisymmetrize", " ", "ERI"}], "*)"}], "\n", 
      RowBox[{"soERI", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"soERI", "\[LeftDoubleBracket]", 
           RowBox[{"p", ",", "q", ",", "r", ",", "s"}], 
           "\[RightDoubleBracket]"}], "-", 
          RowBox[{"soERI", "\[LeftDoubleBracket]", 
           RowBox[{"p", ",", "q", ",", "s", ",", "r"}], 
           "\[RightDoubleBracket]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"p", ",", "nBas2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"q", ",", "nBas2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"r", ",", "nBas2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"s", ",", "nBas2"}], "}"}]}], "]"}]}], ";", "\n", "\n", 
      RowBox[{"\[Epsilon]O", "=", 
       RowBox[{"so\[Epsilon]", "\[LeftDoubleBracket]", 
        RowBox[{"1", ";;", "nO2"}], "\[RightDoubleBracket]"}]}], ";", 
      RowBox[{"\[Epsilon]V", "=", 
       RowBox[{"so\[Epsilon]", "\[LeftDoubleBracket]", 
        RowBox[{
         RowBox[{"nO2", "+", "1"}], ";;", "nBas2"}], 
        "\[RightDoubleBracket]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"\[CapitalDelta]", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
          "\[Epsilon]V", "\[LeftDoubleBracket]", "a", 
           "\[RightDoubleBracket]"}], "+", 
          RowBox[{
          "\[Epsilon]V", "\[LeftDoubleBracket]", "b", 
           "\[RightDoubleBracket]"}], "-", 
          RowBox[{
          "\[Epsilon]O", "\[LeftDoubleBracket]", "i", 
           "\[RightDoubleBracket]"}], "-", 
          RowBox[{
          "\[Epsilon]O", "\[LeftDoubleBracket]", "j", 
           "\[RightDoubleBracket]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"b", ",", "nV2"}], "}"}]}], "]"}]}], ";", "\n", "\n", 
      RowBox[{"OOOO", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"soERI", "\[LeftDoubleBracket]", 
          RowBox[{"i", ",", "j", ",", "k", ",", "l"}], 
          "\[RightDoubleBracket]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"l", ",", "nO2"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"OOVV", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"soERI", "\[LeftDoubleBracket]", 
          RowBox[{"i", ",", "j", ",", 
           RowBox[{"nO2", "+", "a"}], ",", 
           RowBox[{"nO2", "+", "b"}]}], "\[RightDoubleBracket]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"b", ",", "nV2"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"OVOV", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"soERI", "\[LeftDoubleBracket]", 
          RowBox[{"i", ",", 
           RowBox[{"nO2", "+", "a"}], ",", "j", ",", 
           RowBox[{"nO2", "+", "b"}]}], "\[RightDoubleBracket]"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"b", ",", "nV2"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"OVVO", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"soERI", "\[LeftDoubleBracket]", 
