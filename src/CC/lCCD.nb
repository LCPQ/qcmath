       "]"}], ";", "\n", "\n", 
      RowBox[{"(*", "options", "*)"}], "\n", 
      RowBox[{"maxSCF", "=", 
       RowBox[{"options", "[", "\"\<max_SCF_CC\>\"", "]"}]}], ";", "\n", 
      RowBox[{"thresh", "=", 
       RowBox[{"options", "[", "\"\<thresh_CC\>\"", "]"}]}], ";", "\n", 
