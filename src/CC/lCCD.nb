           RowBox[{"nO2", "+", "a"}], ",", 
           RowBox[{"nO2", "+", "b"}], ",", "j"}], "\[RightDoubleBracket]"}], 
         ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "nO2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"b", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "nO2"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"VVVV", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"soERI", "\[LeftDoubleBracket]", 
          RowBox[{
           RowBox[{"nO2", "+", "a"}], ",", 
           RowBox[{"nO2", "+", "b"}], ",", 
           RowBox[{"nO2", "+", "c"}], ",", 
           RowBox[{"nO2", "+", "d"}]}], "\[RightDoubleBracket]"}], ",", 
         RowBox[{"{", 
          RowBox[{"a", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"b", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"c", ",", "nV2"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"d", ",", "nV2"}], "}"}]}], "]"}]}], ";", "\n", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"MP2", " ", "guess", " ", "amplitudes"}], " ", "*)"}], "\n", 
      RowBox[{"t", "=", 
       RowBox[{"-", 
        FractionBox["OOVV", "\[CapitalDelta]"]}]}], ";", "\n", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{"Iterative", " ", "lCCD", " ", "algorithm"}], " ", "*)"}], 
      "\n", 
      RowBox[{"Print", "[", 
       RowBox[{"TableForm", "[", 
        RowBox[{"{", 
         RowBox[{"{", 
          RowBox[{
          "\"\<It.\>\"", ",", 
           "\"\<\!\(\*SuperscriptBox[\(E\), \(lCCD\)]\)               \>\"", 
           ",", "\"\<\!\(\*SubsuperscriptBox[\(E\), \(c\), \(lCCD\)]\)        \
\>\"", ",", "\"\<Conv\>\""}], "}"}], "}"}], "]"}], "]"}], ";", 
      "\[IndentingNewLine]", "\n", 
      RowBox[{"Conv", "=", "1"}], ";", "\t", 
      RowBox[{"nSCF", "=", "0"}], ";", "\t", 
      RowBox[{"EclCCD", "=", "0"}], ";", "\n", "\n", 
      RowBox[{"(*", "Initialization", "*)"}], "\n", 
      RowBox[{"nDIIS", "=", "0"}], ";", "\n", 
      RowBox[{"FDIIS", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"nO2", "*", "nO2", "*", "nV2", "*", "nV2"}]}], "}"}], ",", 
         
         RowBox[{"{", 
          RowBox[{"j", ",", "maxDIIS"}], "}"}]}], "]"}]}], ";", "\n", 
      RowBox[{"eDIIS", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", 
           RowBox[{"nO2", "*", "nO2", "*", "nV2", "*", "nV2"}]}], "}"}], ",", 
         
         RowBox[{"{", 
          RowBox[{"j", ",", "maxDIIS"}], "}"}]}], "]"}]}], ";", "\n", "\n", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Conv", ">", "thresh"}], "\[And]", 
         RowBox[{"nSCF", "<", "maxSCF"}]}], ",", "\[IndentingNewLine]", "  ", 
        
        RowBox[{"(*", " ", "Increment", " ", "*)"}], "\[IndentingNewLine]", 
        "  ", 
        RowBox[{
         RowBox[{"nSCF", "++"}], ";", "\n", "  ", "\n", "  ", 
         RowBox[{"(*", 
          RowBox[{"Compute", " ", "residual"}], "*)"}], "\n", "  ", 
         RowBox[{"y", "=", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{
             UnderoverscriptBox["\[Sum]", 
              RowBox[{"c", "=", "1"}], "nV2"], 
             RowBox[{
              UnderoverscriptBox["\[Sum]", 
               RowBox[{"d", "=", "1"}], "nV2"], 
              RowBox[{
               RowBox[{"t", "\[LeftDoubleBracket]", 
                RowBox[{"i", ",", "j", ",", "c", ",", "d"}], 
                "\[RightDoubleBracket]"}], "*", 
               RowBox[{"OOVV", "\[LeftDoubleBracket]", 
                RowBox[{"k", ",", "l", ",", "c", ",", "d"}], 
                "\[RightDoubleBracket]"}]}]}]}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "nO2"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "nO2"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"k", ",", "nO2"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"l", ",", "nO2"}], "}"}]}], "]"}]}], ";", "\n", "  ", 
         RowBox[{"r2", "=", "\n", "  ", 
          RowBox[{"Table", "[", "\n", "    ", 
           RowBox[{
            RowBox[{
             RowBox[{
              FractionBox["1", "2"], 
              RowBox[{
               UnderoverscriptBox["\[Sum]", 
                RowBox[{"k", "=", "1"}], "nO2"], 
               RowBox[{
                UnderoverscriptBox["\[Sum]", 
                 RowBox[{"l", "=", "1"}], "nO2"], 
                RowBox[{
                 RowBox[{"t", "\[LeftDoubleBracket]", 
                  RowBox[{"k", ",", "l", ",", "a", ",", "b"}], 
                  "\[RightDoubleBracket]"}], "*", 
                 RowBox[{"OOOO", "\[LeftDoubleBracket]", 
                  RowBox[{"i", ",", "j", ",", "k", ",", "l"}], 
