             RowBox[{
              FractionBox["1", "2"], 
              RowBox[{
               UnderoverscriptBox["\[Sum]", 
                RowBox[{"c", "=", "1"}], "nV2"], 
               RowBox[{
                UnderoverscriptBox["\[Sum]", 
                 RowBox[{"d", "=", "1"}], "nV2"], 
                RowBox[{
                 RowBox[{"VVVV", "\[LeftDoubleBracket]", 
                  RowBox[{"c", ",", "d", ",", "a", ",", "b"}], 
                  "\[RightDoubleBracket]"}], "*", 
                 RowBox[{"t", "\[LeftDoubleBracket]", 
                  RowBox[{"i", ",", "j", ",", "c", ",", "d"}], 
                  "\[RightDoubleBracket]"}]}]}]}]}], "+", "\n", "    ", 
             RowBox[{
              FractionBox["1", "4"], 
              RowBox[{
               UnderoverscriptBox["\[Sum]", 
                RowBox[{"k", "=", "1"}], "nO2"], 
               RowBox[{
                UnderoverscriptBox["\[Sum]", 
                 RowBox[{"l", "=", "1"}], "nO2"], 
                RowBox[{
                 RowBox[{"y", "\[LeftDoubleBracket]", 
                  RowBox[{"i", ",", "j", ",", "k", ",", "l"}], 
                  "\[RightDoubleBracket]"}], "*", 
                 RowBox[{"t", "\[LeftDoubleBracket]", 
                  RowBox[{"k", ",", "l", ",", "a", ",", "b"}], 
                  "\[RightDoubleBracket]"}]}]}]}]}]}], "\n", "    ", ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "nO2"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "nO2"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"a", ",", "nV2"}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"b", ",", "nV2"}], "}"}]}], "\n", "  ", "]"}]}], ";", 
         "\n", "  ", "\n", "  ", 
         RowBox[{"r2", "=", 
          RowBox[{"OOVV", "+", 
           RowBox[{"\[CapitalDelta]", "*", "t"}], "+", "r2"}]}], ";", "\n", 
         "  ", "\n", "  ", 
         RowBox[{"(*", 
          RowBox[{"Check", " ", "convergence"}], "*)"}], "\n", "  ", 
         RowBox[{"Conv", "=", 
          RowBox[{"Max", "[", 
           RowBox[{"Abs", "[", "r2", "]"}], "]"}]}], ";", "\n", "  ", "\n", 
         "  ", 
         RowBox[{"(*", 
          RowBox[{"Update", " ", "amplitudes"}], "*)"}], "\n", "  ", 
         RowBox[{"t", "=", 
          RowBox[{"t", "-", 
           FractionBox["r2", "\[CapitalDelta]"]}]}], ";", "\n", "  ", "\n", 
         "  ", 
         RowBox[{"(*", 
          RowBox[{"Compute", " ", "correlation", " ", "energy"}], "*)"}], 
         "\n", "  ", 
         RowBox[{"EclCCD", "=", 
          RowBox[{
           FractionBox["1", "4"], 
           RowBox[{
            UnderoverscriptBox["\[Sum]", 
             RowBox[{"i", "=", "1"}], "nO2"], 
            RowBox[{
             UnderoverscriptBox["\[Sum]", 
              RowBox[{"j", "=", "1"}], "nO2"], 
             RowBox[{
              UnderoverscriptBox["\[Sum]", 
               RowBox[{"a", "=", "1"}], "nV2"], 
              RowBox[{
               UnderoverscriptBox["\[Sum]", 
                RowBox[{"b", "=", "1"}], "nV2"], 
               RowBox[{
                RowBox[{"OOVV", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j", ",", "a", ",", "b"}], 
                 "\[RightDoubleBracket]"}], "*", 
                RowBox[{"t", "\[LeftDoubleBracket]", 
                 RowBox[{"i", ",", "j", ",", "a", ",", "b"}], 
                 "\[RightDoubleBracket]"}]}]}]}]}]}]}]}], ";", "\n", "  ", 
         "\n", "  ", 
         RowBox[{"ElCCD", "=", 
          RowBox[{"(*", 
           RowBox[{"ENuc", "+"}], "*)"}], 
          RowBox[{"EHF", "+", "EclCCD"}]}], ";", "\n", "  ", "\n", "  ", 
         RowBox[{"If", "[", 
          RowBox[{"DIISEx", ",", "\n", "    ", 
