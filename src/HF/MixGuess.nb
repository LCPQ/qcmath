 RowBox[{
  RowBox[{
   RowBox[{"Mix", "[", 
    RowBox[{
    "n\[Alpha]_", ",", "n\[Beta]_", ",", "Ca_", ",", "Cb_", ",", 
     "\[Theta]_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "HOMO", ",", "LUMO", ",", "HOMOa", ",", "HOMOb", ",", "LUMOa", ",", 
       "LUMOb", ",", "newCa", ",", "newCb", ",", "Orba", ",", "Orbb", ",", 
       "Mixquantities"}], "}"}], ",", "\n", "\n", 
     RowBox[{
      RowBox[{"HOMO", "=", 
       RowBox[{"Ca", "\[LeftDoubleBracket]", 
        RowBox[{";;", ",", "n\[Alpha]"}], "\[RightDoubleBracket]"}]}], ";", 
      "\n", 
      RowBox[{"LUMO", "=", 
       RowBox[{"Ca", "\[LeftDoubleBracket]", 
        RowBox[{";;", ",", 
         RowBox[{"n\[Alpha]", "+", "1"}]}], "\[RightDoubleBracket]"}]}], ";", 
      "\n", "\n", 
      RowBox[{"newCa", "=", "Ca"}], ";", "\n", 
      RowBox[{"newCb", "=", "Cb"}], ";", "\n", "\n", 
      RowBox[{"HOMOa", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "HOMO"}], " ", "+", " ", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "LUMO"}]}]}], ";", "\n", 
      RowBox[{"HOMOb", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "HOMO"}], " ", "-", " ", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "LUMO"}]}]}], ";", "\n", "\n", 
      RowBox[{"LUMOa", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"-", 
          RowBox[{"Sin", "[", "\[Theta]", "]"}]}], "HOMO"}], " ", "+", " ", 
        RowBox[{
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "LUMO"}]}]}], ";", "\n", 
      RowBox[{"LUMOb", " ", "=", " ", 
       RowBox[{
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "HOMO"}], " ", "+", " ", 
        RowBox[{
         RowBox[{"Cos", "[", "\[Theta]", "]"}], "LUMO"}]}]}], ";", "\n", "\n",
       "\n", 
      RowBox[{
       RowBox[{"newCa", "\[LeftDoubleBracket]", 
        RowBox[{";;", ",", "n\[Alpha]"}], "\[RightDoubleBracket]"}], "=", 
       "HOMOa"}], ";", "\n", 
      RowBox[{
       RowBox[{"newCa", "\[LeftDoubleBracket]", 
        RowBox[{";;", ",", 
         RowBox[{"n\[Alpha]", "+", "1"}]}], "\[RightDoubleBracket]"}], "=", 
       "LUMOa"}], ";", "\n", "\n", 
      RowBox[{"Orba", "=", 
       RowBox[{
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"newCa", "\[Transpose]"}], ",", "n\[Alpha]"}], "]"}], 
        "\[Transpose]"}]}], ";", "\n", "\n", 
      RowBox[{
       RowBox[{"newCb", "\[LeftDoubleBracket]", 
        RowBox[{";;", ",", "n\[Beta]"}], "\[RightDoubleBracket]"}], "=", 
       "HOMOb"}], ";", "\n", 
      RowBox[{
       RowBox[{"newCb", "\[LeftDoubleBracket]", 
        RowBox[{";;", ",", 
         RowBox[{"n\[Beta]", "+", "1"}]}], "\[RightDoubleBracket]"}], "=", 
       "LUMOb"}], ";", "\n", "\n", 
      RowBox[{"Orbb", "=", 
       RowBox[{
        RowBox[{"Take", "[", 
         RowBox[{
          RowBox[{"newCb", "\[Transpose]"}], ",", "n\[Beta]"}], "]"}], 
        "\[Transpose]"}]}], ";", "\n", "\n", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Mixquantities", "=", 
         RowBox[{"Association", "[", 
          RowBox[{
           RowBox[{"\"\<Orba\>\"", "\[Rule]", "Orba"}], ",", 
           RowBox[{"\"\<Orbb\>\"", "\[Rule]", "Orbb"}]}], "]"}]}], ";"}], 
       "*)"}], "\n", "\n", 
      RowBox[{"Return", "[", 
       RowBox[{"{", 
        RowBox[{"Orba", ",", "Orbb"}], "}"}], "]"}]}]}], "]"}]}], 
