Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"readMolecule", "[", "molecule_", "]"}], ":=", "\n", "  ", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
      "mol", ",", "xyz", ",", "nAt", ",", "elements", ",", "Znuc", ",", 
       "sfile", ",", "Enuc", ",", "outputs"}], "}"}], ",", "\n", "\n", "    ", 
     RowBox[{"(*", 
      RowBox[{
      "Get", " ", "the", " ", "molecule", " ", "file", " ", "with", " ", "a", 
       " ", "list", " ", "of", " ", 
       RowBox[{"atom", "'"}], "s", " ", "symbols"}], "*)"}], "\n", "    ", 
     RowBox[{
      RowBox[{"mol", "=", 
       RowBox[{"Import", "[", 
        RowBox[{
         RowBox[{
         "path", "<>", "\"\</mol/\>\"", "<>", "molecule", "<>", 
          "\"\<.xyz\>\""}], ",", "\"\<VertexTypes\>\""}], "]"}]}], ";", "\n", 
      "    ", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"SameQ", "[", 
          RowBox[{"mol", ",", "$Failed"}], "]"}], "\[Equal]", "True"}], ",", 
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"Style", "[", 
           RowBox[{"\"\<xyz file not found\>\"", ",", "Bold", ",", "Red"}], 
           "]"}], "]"}], ";", 
         RowBox[{"Abort", "[", "]"}]}]}], "]"}], ";", "\n", "    ", "\n", 
      "    ", 
      RowBox[{"(*", 
       RowBox[{"Get", " ", "the", " ", "geometry"}], "*)"}], "\n", "    ", 
      RowBox[{"xyz", "=", 
       RowBox[{"Import", "[", 
        RowBox[{
         RowBox[{
         "path", "<>", "\"\</mol/\>\"", "<>", "molecule", "<>", 
          "\"\<.xyz\>\""}], ",", "\"\<VertexCoordinates\>\""}], "]"}]}], ";", 
      "\n", "\n", "    ", 
      RowBox[{"(*", 
       RowBox[{"Number", " ", "of", " ", "atoms"}], "*)"}], "\n", "    ", 
      RowBox[{"nAt", "=", 
       RowBox[{"Length", "[", "mol", "]"}]}], ";", "\n", "\n", "    ", 
      RowBox[{"(*", 
       RowBox[{
       "List", " ", "of", " ", "atoms", " ", "and", " ", "their", " ", 
        "atomic", " ", "numbers"}], "*)"}], "\n", "    ", 
      RowBox[{"elements", "=", 
       RowBox[{"Association", "[", "\n", "               ", 
        RowBox[{
         RowBox[{"\"\<H\>\"", "\[Rule]", "1"}], ",", 
         RowBox[{"\"\<He\>\"", "\[Rule]", "2"}], ",", 
         RowBox[{"\"\<Li\>\"", "\[Rule]", "3"}], ",", 
         RowBox[{"\"\<Be\>\"", "\[Rule]", "4"}], ",", 
         RowBox[{"\"\<B\>\"", "\[Rule]", "5"}], ",", 
         RowBox[{"\"\<C\>\"", "\[Rule]", "6"}], ",", 
         RowBox[{"\"\<N\>\"", "\[Rule]", "7"}], ",", "\[IndentingNewLine]", 
         "               ", 
         RowBox[{"\"\<O\>\"", "\[Rule]", "8"}], ",", 
         RowBox[{"\"\<F\>\"", "\[Rule]", "9"}], ",", 
         RowBox[{"\"\<Ne\>\"", "\[Rule]", "10"}], ",", 
         RowBox[{"\"\<Na\>\"", "\[Rule]", "11"}], ",", 
         RowBox[{"\"\<Mg\>\"", "\[Rule]", "12"}], ",", 
         RowBox[{"\"\<Al\>\"", "\[Rule]", "13"}], ",", "\n", 
         "               ", 
         RowBox[{"\"\<Si\>\"", "\[Rule]", "14"}], ",", 
         RowBox[{"\"\<P\>\"", "\[Rule]", "15"}], ",", 
         RowBox[{"\"\<S\>\"", "\[Rule]", "16"}], ",", 
         RowBox[{"\"\<Cl\>\"", "\[Rule]", "17"}], ",", 
         RowBox[{"\"\<Ar\>\"", "\[Rule]", "18"}]}], "\n", "             ", 
        "]"}]}], ";", "\n", "\n", "    ", 
      RowBox[{"(*", 
       RowBox[{
       "Attribute", " ", "the", " ", "right", " ", "atomic", " ", "number", 
        " ", "to", " ", "the", " ", "atoms"}], "*)"}], "\n", "    ", 
      RowBox[{"Znuc", "=", 
       RowBox[{"Table", "[", 
        RowBox[{"0", ",", 
         RowBox[{"{", 
          RowBox[{"N", ",", 
           RowBox[{"Length", "[", "mol", "]"}]}], "}"}]}], "]"}]}], ";", "\n",
       "    ", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
         "Znuc", "\[LeftDoubleBracket]", "N", "\[RightDoubleBracket]"}], "=", 
         
         RowBox[{"Replace", "[", 
          RowBox[{
           RowBox[{
           "mol", "\[LeftDoubleBracket]", "N", "\[RightDoubleBracket]"}], ",",
            "elements"}], "]"}]}], ",", 
        RowBox[{"{", 
         RowBox[{"N", ",", 
          RowBox[{"Length", "[", "mol", "]"}]}], "}"}]}], "]"}], ";", "\n", 
      "\n", "    ", 
      RowBox[{"(*", 
       RowBox[{"Definition", " ", "of", " ", "the", " ", 
        RowBox[{"atom", "/", "molecule"}], " ", "geometry", " ", "in", " ", 
        "the", " ", 
        RowBox[{"mol", ".", "xyz"}], " ", "file"}], "*)"}], "\n", "\n", 
      "    ", 
      RowBox[{"sfile", "=", 
       RowBox[{"OpenWrite", "[", 
        RowBox[{
         RowBox[{"path", "<>", "\"\</mol/mol.xyz\>\""}], ",", 
         RowBox[{"FormatType", "\[Rule]", "StandardForm"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "    ", 
      RowBox[{"Write", "[", 
       RowBox[{"sfile", ",", "nAt"}], "]"}], ";", "\[IndentingNewLine]", 
      "    ", 
      RowBox[{"Write", "[", 
       RowBox[{"sfile", ",", "\"\<\>\""}], "]"}], ";", "\n", "    ", 
      RowBox[{"Do", "[", 
       RowBox[{
        RowBox[{"Write", "[", 
         RowBox[{"sfile", ",", 
          RowBox[{
          "mol", "\[LeftDoubleBracket]", "i", "\[RightDoubleBracket]"}], ",", 
          "\"\< \>\"", ",", 
          RowBox[{"Row", "[", 
           RowBox[{
            RowBox[{"xyz", "\[LeftDoubleBracket]", 
             RowBox[{"i", ",", ";;"}], "\[RightDoubleBracket]"}], ",", 
            "\"\< \>\""}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "nAt"}], "}"}]}], "]"}], ";", "\n", "    ", 
      RowBox[{"Close", "[", "sfile", "]"}], ";", "\n", "\n", "\n", "    ", 
      RowBox[{"(*", 
       RowBox[{"nuclear", " ", "energy"}], "*)"}], "\n", "    ", 
      RowBox[{"Enuc", "=", 
       RowBox[{
        UnderoverscriptBox["\[Sum]", 
         RowBox[{"c", "=", "1"}], "nAt"], 
        RowBox[{
         UnderoverscriptBox["\[Sum]", 
          RowBox[{"d", "=", 
           RowBox[{"c", "+", "1"}]}], "nAt"], 
         FractionBox[
          RowBox[{
           RowBox[{
           "Znuc", "\[LeftDoubleBracket]", "c", "\[RightDoubleBracket]"}], 
           " ", 
           RowBox[{
           "Znuc", "\[LeftDoubleBracket]", "d", "\[RightDoubleBracket]"}]}], 
          RowBox[{
           RowBox[{"Norm", "[", 
            RowBox[{
             RowBox[{
             "xyz", "\[LeftDoubleBracket]", "c", "\[RightDoubleBracket]"}], 
             "-", 
             RowBox[{
             "xyz", "\[LeftDoubleBracket]", "d", "\[RightDoubleBracket]"}]}], 
            "]"}], "*", "AntoBo"}]]}]}]}], ";", "\n", "\n", "    ", 
      RowBox[{"outputs", " ", "=", " ", 
       RowBox[{"Association", "[", 
        RowBox[{
         RowBox[{"\"\<mol\>\"", "\[Rule]", "mol"}], ",", 
         RowBox[{"\"\<xyz\>\"", "\[Rule]", "xyz"}], ",", 
         RowBox[{"\"\<nAt\>\"", "\[Rule]", "nAt"}], ",", 
         RowBox[{"\"\<Enuc\>\"", "\[Rule]", "Enuc"}]}], "]"}]}], ";", "\n", 
      "\n", "    ", 
      RowBox[{"Return", "[", "outputs", "]"}], ";"}]}], "\n", "  ", "]"}]}], 
  ";"}]], "Code",
